<h1>Events</h1>

<h2>Upcoming</h2>
<div class="events-container">

  <% @events.upcoming.each do |event| %>
    <%= render 'events/event', event: event %>
  <% end %>
</div>

<h2>Previous</h2>
<div class="events-container">
  <% @events.previous.each do |event| %>
    <%= render 'events/event', event: event %>
  <% end %>

</div>